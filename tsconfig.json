{
	"compilerOptions": {
		"target": "ESNext",
		"module": "NodeNext",
		"moduleResolution": "NodeNext",
		"esModuleInterop": true,
		"forceConsistentCasingInFileNames": true,
		"strict": true,
		"skipLibCheck": true,
		"types": ["@cloudflare/workers-types/2023-07-01", "@types/node"],
		"noEmit": true,
		"isolatedModules": true
	}
}
